.banner {
  width: 100%;
  height: 340rpx;
}
.banner image {
  width: 100%;
  height: 340rpx;
}
.navs {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 44rpx;
}
.navs image {
  width: 128rpx;
  height: 140rpx;
}
.floor .title {
  width: 750rpx;
  height: 80rpx;
  padding: 20rpx 0 0 8rpx;
  box-sizing: border-box;
  background-color: #f4f4f4;
}
.floor .items {
  padding: 20rpx 16rpx;
  overflow: hidden;
}
.floor .items navigator {
  width: 193rpx;
  height: 188rpx;
  margin-left: 10rpx;
  margin-bottom: 10rpx;
  float: left;
}
.floor .items navigator:first-child {
  width: 232rpx;
  height: 386rpx;
  margin-left: 0rpx;
}
.floor .items navigator:nth-child(2),
.floor .items navigator:nth-child(5) {
  width: 273rpx;
}
.floor:first-child .items navigator {
  width: 233rpx;
}

